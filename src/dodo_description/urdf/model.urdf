<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="dodo_model">
     
    <link name="base_link">
        
    </link>

     <link name="body_link">
        <inertial>
            <origin xyz="0.0 0.0 0.45" rpy="0.0 0.0 0.0"/>
            <mass value="15.0"/> <!-- Base mass -->
            <inertia ixx="1.266" ixy="0.0" ixz="0.0" iyy="1.266" iyz="0.0" izz="0.507"/>
        </inertial>
        <visual name="">
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <geometry>
                <mesh filename="package://dodo_description/meshes/Cylinder.stl" scale="0.26 0.26 0.12844"/>
            </geometry>
            <material name="">
                <color rgba="1.0 0.0 0.0 1.0"/>
                <texture filename=""/>
            </material>
        </visual>
        <collision>
            <origin xyz="0.0 0.0 0.45" rpy="0.0 0.0 0.0"/>
            <geometry>
                <cylinder radius="0.27358" length="0.89942"/>
            </geometry>
        </collision>
     </link>

     <joint name="body_link_joint" type="fixed">
        <parent link="base_link"/>
        <child link="body_link"/>
        <origin xyz="0.0 0.0 0.035" rpy="0.0 0.0 0.0"/>
     </joint>

     <link name="lidar">
        <inertial>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <mass value="0.0"/>
            <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
        </inertial>
        <visual name="">
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <geometry>
                <cylinder radius="0.03" length="0.03"/>
            </geometry>
            <material name="">
                <color rgba="1.0 0.0 0.0 1.0"/>
                <texture filename=""/>
            </material>
        </visual>
        <collision>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <geometry>
                <box size="0.0 0.0 0.0"/>
            </geometry>
        </collision>
     </link>

     <joint name="lidar_joint" type="fixed">
        <origin xyz="0.0 0.0 0.2" rpy="0.0 0.0 0.0"/>
        <parent link="body_link"/>
        <child link="lidar"/>
        <axis xyz="0.0 0.0 0.0"/>
        <limit lower="0.0" upper="0.0" effort="0.0" velocity="0.0"/>
     </joint>



     <gazebo>
    <plugin name="differential_drive_controller" filename="libgazebo_ros_diff_drive.so">
        <leftJoint>left_wheel_joint</leftJoint>
        <rightJoint>right_wheel_joint</rightJoint>
        <wheelSeparation>0.38</wheelSeparation> <!-- Distance between wheels -->
        <wheelDiameter>0.08</wheelDiameter> <!-- Wheel diameter -->
        <torque>6</torque>
        <commandTopic>/cmd_vel</commandTopic>
        <odometryTopic>/odom</odometryTopic>
        <odometryFrame>odom</odometryFrame>
        <robotBaseFrame>base_link</robotBaseFrame>
        <publishWheelTF>true</publishWheelTF>
        <publishWheelJointState>true</publishWheelJointState>
    </plugin>
</gazebo>





 <gazebo reference="lidar">
    <sensor type="ray" name="head_hokuyo_sensor">
        <pose>0 0 0 0 0 0</pose>
        <visualize>true</visualize>
        <update_rate>10</update_rate>
        <ray>
            <scan>
                <horizontal>
                    <samples>720</samples>
                    <resolution>1</resolution>
                    <min_angle>-1.570796</min_angle>
                    <max_angle>1.570796</max_angle>
                </horizontal>
            </scan>
            <range>
                <min>0.40</min> <!-- Adjusted minimum range -->
                <max>10.0</max>
                <resolution>0.01</resolution>
            </range>
            <noise>
                <type>gaussian</type>
                <mean>0.0</mean>
                <stddev>0.01</stddev>
            </noise>
        </ray>
        <plugin name="gazebo_ros_laser_controller" filename="libgazebo_ros_laser.so">
            <topicName>/scan</topicName> <!-- Ensure this matches the topic name -->
            <frameName>lidar</frameName> <!-- Ensure this matches the link name -->
        </plugin>
    </sensor>
</gazebo>


  <link name="oak_link">
    <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
            <box size="0.05 0.06 0.03"/>
        </geometry>
        <material name="">
            <color rgba="1.0 0.0 0.0 1.0"/>
        </material>
    </visual>
</link>


<joint name="oak_link_joint" type="fixed">
    <origin xyz="0.25 0.0 0.11" rpy="0.0 0.0 0.0"/>
    <parent link="body_link"/>
    <child link="oak_link"/>
    <axis xyz="0.0 0.0 0.0"/>
    <limit lower="0.0" upper="0.0" effort="0.0" velocity="0.0"/>
</joint>




<gazebo reference="oak_link">
    <sensor type="camera" name="oak_camera">
        <update_rate>30</update_rate>
        <camera>
            <horizontal_fov>1.396</horizontal_fov>
            <image>
                <width>640</width>
                <height>480</height>
                <format>R8G8B8</format>
            </image>
            <clip>
                <near>0.02</near>
                <far>100.0</far>
            </clip>
        </camera>
        <plugin name="camera_controller" filename="libgazebo_ros_camera.so">
            <alwaysOn>true</alwaysOn>
            <updateRate>30</updateRate>
            <cameraName>robot_camera</cameraName>
            <imageTopicName>/camera/image_raw</imageTopicName>
            <cameraInfoTopicName>/camera/camera_info</cameraInfoTopicName>
            <frameName>camera_link</frameName>
        </plugin>
    </sensor>
</gazebo>



     <link name="right_wheel">
        <inertial>
            <mass value="1.0"/> <!-- Adjusted mass -->
            <inertia ixx="0.0008" ixy="0.0" ixz="0.0" iyy="0.0005" iyz="0.0" izz="0.001"/>
        </inertial>
        <visual name="">
            <origin xyz="0.0 0.0 0.005" rpy="0.0 0.0 0.0"/>
            <geometry>
                <cylinder radius="0.04" length="0.02"/>
            </geometry>
            <material name="">
                <color rgba="1.0 0.0 0.0 1.0"/>
                <texture filename=""/>
            </material>
        </visual>
        <collision>
            <origin xyz="0.0 0.0 0.005" rpy="0.0 0.0 0.0"/>
            <geometry>
                <cylinder radius="0.04" length="0.02"/>
            </geometry>
        </collision>
     </link>

     <joint name="right_wheel_joint" type="continuous">
        <parent link="body_link"/>
        <child link="right_wheel"/>
        <axis xyz="0 0 1"/>
        <origin xyz="0 -0.19 0.005" rpy="-1.6 0 0"/>
        <limit effort="0.0" velocity="0.0"/>
        <dynamics damping="0.1" friction="0.1"/>
     </joint>



     <link name="left_wheel">
        <inertial>
            <mass value="1.0"/> <!-- Adjusted mass -->
            <inertia ixx="0.0008" ixy="0.0" ixz="0.0" iyy="0.0005" iyz="0.0" izz="0.001"/>
        </inertial>
        <visual name="">
            <origin xyz="0.0 0.0 0.005" rpy="0.0 0.0 0.0"/>
            <geometry>
                <cylinder radius="0.04" length="0.02"/>
            </geometry>
            <material name="">
                <color rgba="1.0 0.0 0.0 1.0"/>
                <texture filename=""/>
            </material>
        </visual>
        <collision>
            <origin xyz="0.0 0.0 0.005" rpy="0.0 0.0 0.0"/>
            <geometry>
               <cylinder radius="0.04" length="0.02"/>
            </geometry>
        </collision>
     </link>

     <joint name="left_wheel_joint" type="continuous">
        <parent link="body_link"/>
        <child link="left_wheel"/>
        <axis xyz="0 0 1"/>
        <origin xyz="0 0.19 0.005" rpy="-1.6 0 0"/>
        <limit effort="0.0" velocity="0.0"/>
        <dynamics damping="0.1" friction="0.1"/>
     </joint>

     
   <link name="front_caster">
        <inertial>
          <mass value="0.8"/>
          <inertia ixx="0.00067" ixy="0.0" ixz="0.0" iyy="0.00067" iyz="0.0" izz="0.00100"/>
        </inertial>
        <visual name="">
            <geometry>
                <sphere radius="0.03"/>
            </geometry>
            <material name="">
                <color rgba="1.0 0.0 0.0 1.0"/>
                <texture filename=""/>
            </material>
        </visual>
        <collision>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <geometry>
                 <sphere radius="0.03"/>
            </geometry>
        </collision>
     </link>

     <joint name="front_caster_joint" type="continuous">
        <parent link="body_link"/>
        <child link="front_caster"/>
        <axis xyz="0 0 1"/>
        <origin xyz="0.19276 0 -0.005" rpy="0 0 0"/>
        <dynamics damping="0.2" friction="0.2"/>
     </joint>


      
     <link name="back_caster">
        <inertial>
            <mass value="0.8"/>
            <inertia ixx="0.00067" ixy="0.0" ixz="0.0" iyy="0.00067" iyz="0.0" izz="0.00100"/>
        </inertial>
        <visual name="">
            <geometry>
                <sphere radius="0.03"/>
            </geometry>
            <material name="">
                <color rgba="1.0 0.0 0.0 1.0"/>
                <texture filename=""/>
            </material>
        </visual>
        <collision>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <geometry>
                 <sphere radius="0.03"/>
            </geometry>
        </collision>
     </link>

     <joint name="back_caster_joint" type="continuous">
        <parent link="body_link"/>
        <child link="back_caster"/>
        <axis xyz="0 0 1"/>
        <origin xyz="-0.19276 0 -0.005" rpy="0 0 0"/>
        <dynamics damping="0.2" friction="0.2"/>
     </joint>
 
     
   

     




</robot>